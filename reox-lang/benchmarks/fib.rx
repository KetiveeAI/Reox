// Fibonacci Benchmark - REOX Version
// Tests: recursion, integer arithmetic

fn fib(n: int) -> int {
    if n <= 1 {
        return n;
    }
    return fib(n - 1) + fib(n - 2);
}

fn main() -> int {
    let n: int = 35;
    let result: int = fib(n);
    print("fib(");
    print_int(n);
    print(") = ");
    print_int(result);
    println("");
    return 0;
}
